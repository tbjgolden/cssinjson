{
  "name": "cssinjson",
  "version": "1.0.0-alpha",
  "description": "CSS in JSON - The optimal way to decrease your CSS bytes",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.js",
  "es5": {
    "main": "dist/es5/index.cjs.js",
    "module": "dist/es5/index.esm.js",
    "browser": "dist/es5/index.js"
  },
  "repository": "https://github.com/tbjgolden/cssinjson.git",
  "author": "Tom Golden <tbjgolden@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-proposal-function-sent": "^7.5.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-flow": "^7.0.0",
    "colors": "^1.3.3",
    "husky": "^3.0.5",
    "jsdoc-to-markdown": "^5.0.1",
    "nodemon": "^1.19.2",
    "nyc": "^14.1.1",
    "rollup": "^1.20.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.1.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.10.0",
    "rollup-plugin-terser": "^5.1.1"
  },
  "scripts": {
    "build": "babel src/ -d es5/ && rollup -c",
    "test": "nyc node test/test.js",
    "pretest": "npm run build",
    "start": "nodemon --exec \"npx yarn test\""
  },
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "@babel/preset-flow",
      [
        "@babel/preset-env",
        {
          "exclude": [
            "transform-typeof-symbol"
          ],
          "loose": true
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "node scripts/readme.js && git add -A"
    }
  }
}
